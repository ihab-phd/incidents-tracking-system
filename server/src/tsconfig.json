{
    "compilerOptions": {
        "noImplicitAny": false,
        "target": "es6",
        "module": "commonjs",
        "outDir": "../dist",
        "declaration": false,
        "removeComments": true,
        "noLib": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "listFiles": false
    },
    "exclude": [
        "node_modules",
        "client"
    ]
}
